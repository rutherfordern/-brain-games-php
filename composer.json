{
    "name": "hexlet/code",
    "description": "Brain Games is bundle of mini-games with command-line interface",
    "license": "MIT",
    "bin": [
        "bin/brain-games",
        "bin/brain-even",
        "bin/brain-calc",
        "bin/brain-gcd"
    ],
    "autoload": {
        "files": [
            "src/Cli.php",
            "src/Games/EvenGame.php",
            "src/Games/CalcGame.php",
            "src/Games/GcdGame.php",
            "src/Engine/GameEngine.php"
        ],
        "psr-4": {
            "Hexlet\\Code\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Egor",
            "email": "glavnegor@yandex.ru"
        }
    ],
    "require": {
        "wp-cli/php-cli-tools": "^0.11.15"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*"
    }
}
